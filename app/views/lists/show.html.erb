<div class="container">
  <div class="list">
    <h2><%= @list.name %></h2>
    <div class="list-bookmarks">
      <p><strong>Reviews</strong></p>
      <ul class="list-group">
        <% @list.bookmarks.each do |bookmark| %>
          <li class="list-group-item"><%= bookmark.comment %></li>
        <% end %>
      </ul>
      <br>
    </div>
  </div>

  <%= link_to "Add bookmark", new_list_bookmark_path(@list) %>
  <%= link_to "Back", lists_path %>
</div>
